    <ControlTemplate x:Key="{ComponentResourceKey ResourceId=LAStateNodeTemplateKey, TypeInTargetAssembly={x:Type base:BaseNodeControl}}" TargetType="{x:Type base:BaseNodeControl}">
        <Grid d:DesignWidth="312.5" d:DesignHeight="225">
            <Border x:Name="PART_Shadow" BorderThickness="4" HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Stretch" Width="Auto" CornerRadius="8"/>
            <Grid  Margin="3,3,3,3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>
                <Border Name="Border" BorderBrush="Black" BorderThickness="0" Grid.ColumnSpan="3" Background="#FF4385D9" CornerRadius="8"/>
                <Grid Grid.Column="0" Grid.Row="0">
                    <ContentPresenter x:Name="PART_Content_Left" ContentSource="LeftContent" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Margin="0"/>
