                <ctrls:ImageEx x:Name="PART_Shadow" DrawImageWith9Cells="True" ClipPadding="20,20,20,20" Source="/ResourceLibrary;component/Icons/Graph/RegularNode_shadow.png" Stretch="Fill" Grid.RowSpan="3" IsHitTestVisible="False" VerticalAlignment="Center"/>
                <ctrls:ImageEx Margin="14" DrawImageWith9Cells="True" ClipPadding="10,10,10,10" Source="/ResourceLibrary;component/Icons/Graph/RegularNode_body.png" Stretch="Fill" Grid.RowSpan="3" VerticalAlignment="Center"/>
                <Grid x:Name="PART_HeaderGrid" Margin="14,14,14,0" VerticalAlignment="Stretch" Height="50">
                    <ctrls:ImageEx DrawImageWith9Cells="True" ClipPadding="10,10,10,0" Source="/ResourceLibrary;component/Icons/Graph/RegularNode_title_gloss.png" Stretch="Fill" StretchDirection="UpOnly">
                        <Image.Effect>
                            <effect:ColorBlendEffect x:Name="BlendColorEffect" BlendBrush="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BlendBrush}"/>
                        </Image.Effect>
                    </ctrls:ImageEx>
                    <ctrls:ImageEx DrawImageWith9Cells="True" ClipPadding="10,10,10,0" Source="/ResourceLibrary;component/Icons/Graph/RegularNode_title_highlight.png" Stretch="Fill"/>
                    <Grid HorizontalAlignment="Left">
                        <!--<ctrls:ImageEx x:Name="TitleLabel" DrawImageWith9Cells="True" ClipPadding="10,10,0,0" Source="/ResourceLibrary;component/Icons/Graph/RegularNode_color_spill.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stretch="Fill">
                        </ctrls:ImageEx>-->
                        <ContentPresenter x:Name="PART_HeaderContent" ContentSource="Header" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="Center" Margin="15,0,15,0"/>
                    </Grid>
                </Grid>
                <ContentPresenter x:Name="PART_Content" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Grid.Row="1" Margin="14,0,14,14"/>
                <baseCtrl:CommentControl x:Name="commentControl" DockOffset="14" Comment="{Binding Comment, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top"/>
            </Grid>
            <Rectangle x:Name="PART_BORDER_LEFT" Grid.Row="1" HorizontalAlignment="Right" Width="8" Cursor="SizeWE" Fill="#02000000"/>
            <Rectangle x:Name="PART_BORDER_Right" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Left" Width="8" Cursor="SizeWE" Fill="#02000000"/>
            <Rectangle x:Name="PART_BORDER_TOP" VerticalAlignment="Bottom" Width="Auto" Grid.Column="1" Height="8" Cursor="SizeNS" Fill="#02000000"/>
            <Rectangle x:Name="PART_BORDER_BOTTOM" Grid.Row="2" HorizontalAlignment="Stretch" Width="Auto" VerticalAlignment="Top" Grid.Column="1" Height="8" Cursor="SizeNS" Fill="#02000000"/>
            <Rectangle x:Name="PART_BORDER_LT" HorizontalAlignment="Right" Width="8" VerticalAlignment="Bottom" Height="8" Cursor="SizeNWSE" Fill="#02000000"/>
            <Rectangle x:Name="PART_BORDER_LB" Grid.Row="2" HorizontalAlignment="Right" Width="8" VerticalAlignment="Top" Height="8" Cursor="SizeNESW" Fill="#02000000"/>
            <Rectangle x:Name="PART_BORDER_RT" Grid.Row="0" HorizontalAlignment="Left" Width="8" Grid.Column="2" VerticalAlignment="Bottom" Height="8" Cursor="SizeNESW" Fill="#02000000"/>
            <Rectangle x:Name="PART_BORDER_RB" Grid.Row="2" HorizontalAlignment="Left" Width="8" Grid.Column="2" VerticalAlignment="Top" Height="8" Cursor="SizeNWSE" Fill="#02000000"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="Selected" Value="True">
                <Setter Property="Source" TargetName="PART_Shadow" Value="/ResourceLibrary;component/Icons/Graph/RegularNode_shadow_selected.png"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
