                <Rectangle x:Name="PART_BORDER_RB" Grid.Column="2" Cursor="SizeWE" Fill="#F2FFFFFF" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="3" Height="3" Margin="0,0,1,1"/>
            </Grid>
