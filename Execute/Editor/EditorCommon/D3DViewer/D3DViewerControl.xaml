<UserControl x:Class="EditorCommon.D3DViewer.D3DViewerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:win="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             xmlns:WinFormHost="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
             xmlns:local="clr-namespace:EditorCommon.D3DViewer"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Loaded="UserControl_Loaded">
    <Grid>
        <WinFormHost:WindowsFormsHost x:Name="Changed" SizeChanged="Changed_SizeChanged">
            <win:Panel x:Name="DrawPanel" AutoSize="True"/>
        </WinFormHost:WindowsFormsHost>
    </Grid>
</UserControl>
