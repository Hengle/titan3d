<myTypes:BaseNodeControl x:Name="baseNodeControl" x:Class="MaterialEditor.Controls.Operation.Abs"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:myTypes ="clr-namespace:CodeGenerateSystem.Base;assembly=CodeGenerateSystem"
             xmlns:baseCtrl="clr-namespace:CodeGenerateSystem.Controls;assembly=CodeGenerateSystem"
             mc:Ignorable="d" BlendBrush="#FF62835D" Header="Abs" Foreground="White">
    <myTypes:BaseNodeControl.Resources>
        <ResourceDictionary Source="/MaterialEditor;component/Themes/Generic.xaml" />
    </myTypes:BaseNodeControl.Resources>
    <Grid>
        <baseCtrl:LinkInControl Name="InputLink" Margin="8,0,0,0" Grid.Row="1" BackBrush="{StaticResource LinkDefault}" HorizontalAlignment="Left" Direction="Left" VerticalAlignment="Center" />
        <baseCtrl:LinkOutControl Name="ResultLink" Margin="0,0,8,0" Grid.Row="1" BackBrush="{StaticResource LinkDefault}" HorizontalAlignment="Right" Direction="Right" VerticalAlignment="Center" />
    </Grid>
</myTypes:BaseNodeControl>
